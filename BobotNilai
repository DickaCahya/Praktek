package Percabangan;
import java.util.Scanner;
/**
 *
 * @author LENOVO
 */
public class BobotNilaiHuruf {
     public static void main(String[] args){
     String huruf;
     Scanner scan = new Scanner(System.in);
 
  System.out.print("Masukkan Huruf [A,B,C,D,E] = ");
  huruf = scan.nextLine();
  
  switch(huruf){
   case "A" :
    System.out.println("4");
    break;
   case "B" :
    System.out.println("3");
    break;
   case "C" :
    System.out.println("2");
    break;
   case "D" :
    System.out.println("1");
    break;
   case "E" :
    System.out.println("0");
    break;
   default:
    System.out.println("huruf yang anda masukan salah");
  }
     }
    
     
}
